<div class="container">
    <div class="row">
        <div class="col-md-3" id="card-profile">
            <div class="my-list">
                <h1 class="add-address">
                    <span>+</span>
                    <span>
                        <h2>Add Image</h2>
                    </span>
                </h1>
            </div>
        </div>

        <div class="col-md-6" id="card-profile">
            <div class="my-list">

                <h2>User Information</h2>
                <input class="input-field" type="text" placeholder="User Name" name="usrnm" readonly="true"
                    id="userdetail" value="{{user.username}}">
                <input class="input-field" type="text" placeholder="User Email" name="email" readonly="true"
                    id="userdetail" value="{{user.email}}">
                <input class="input-field" type="text" placeholder="User Phone Number" name="phonenumber"
                    readonly="true" id="userdetail" value="{{user.mobile_number}}">
                <input class="input-field" type="text" placeholder="User Gender" name="gender" readonly="true"
                    id="userdetail" value="">
                <input class="input-field" type="text" placeholder="User Date of Birth" name="dob" readonly="true"
                    id="userdetail" value="">
                <div class="detail">
                    <p><strong>
                            <h2>More Actions</h2>
                        </strong></p>
                    <a href="#" class="btn btn-info">Edit User Information</a><br><br>
                    <a href="#" class="btn btn-warning">Delete User Information</a>
                </div><br><br>
            </div>
        </div>


    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="row" style="margin-top: 27px;">
                <div class="col-md-12">
                    <div class="row">
                        {{#each user.address}}


                        <div class=" col-md-3" id="card2">
                            <div class="my-list">

                                <h2>Address 1</h2>
                                <input class="input-field" type="text" placeholder="Name" name="address"
                                    readonly="true" id="userdetail" value="{{this.name}}">
                                <input class="input-field" type="text" placeholder="Number" name="address"
                                    readonly="true" id="userdetail" value="{{this.number}}">
                                <input class="input-field" type="text" placeholder="Area & Street" name="address"
                                    readonly="true" id="userdetail" value="{{this.user_address}}">
                                <input class="input-field" type="text" placeholder="Pincode" name="address"
                                    readonly="true" id="userdetail" value="{{this.pincode}}">
                                <input class="input-field" type="text" placeholder="Locality" name="address"
                                    readonly="true" id="userdetail" value="{{this.locality}}">
                                <input class="input-field" type="text" placeholder="City" name="address"
                                    readonly="true" id="userdetail" value="{{this.city}}">
                                <input class="input-field" type="text" placeholder="State" name="address"
                                    readonly="true" id="userdetail" value="{{this.state}}">
                                <input class="input-field" type="text" placeholder="Landmark" name="address"
                                    readonly="true" id="userdetail" value="{{this.landmark}}">
                                <input class="input-field" type="text" placeholder="Address Type" name="address"
                                    readonly="true" id="userdetail" value="{{this.add_type}}">

                                <div class="detail">
                                    <p><strong>
                                            <h2>More Actions</h2>
                                        </strong></p>
                                    <a href="#" class="btn btn-info">Edit Address</a><br><br>
                                    <a href="#" class="btn btn-warning">Delete Address</a>
                                </div><br><br>

                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" value="" id="defaultCheck1">
                                    <label class="form-check-label" for="defaultCheck1">
                                        Deliver to this Address
                                    </label>
                                </div>
                            </div>
                        </div>
                        {{/each}}
                        <div class=" col-md-3" id="card2">
                            <div class="my-list">

                                <h2>Address 2</h2>
                                <input class="input-field" type="text" placeholder="Area & Street" name="address"
                                    readonly="true" id="userdetail">
                                <input class="input-field" type="text" placeholder="Pincode" name="address"
                                    readonly="true" id="userdetail">
                                <input class="input-field" type="text" placeholder="Locality" name="address"
                                    readonly="true" id="userdetail">
                                <input class="input-field" type="text" placeholder="City" name="address" readonly="true"
                                    id="userdetail">
                                <input class="input-field" type="text" placeholder="State" name="address"
                                    readonly="true" id="userdetail">
                                <div class="detail">
                                    <p><strong>
                                            <h2>More Actions</h2>
                                        </strong></p>
                                    <a href="#" class="btn btn-info">Edit Address</a><br><br>
                                    <a href="#" class="btn btn-warning">Delete Address</a>
                                </div> <br><br>

                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox" value="" id="defaultCheck1">
                                    <label class="form-check-label" for="defaultCheck1">
                                        Deliver to this Address
                                    </label>
                                </div>
                            </div>
                        </div>
